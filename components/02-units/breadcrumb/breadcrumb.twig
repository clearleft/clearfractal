<div class="Breadcrumb{% if breadcrumbModifier %} Breadcrumb{{breadcrumbModifier}}{% endif %}" role="navigation" aria-label="Breadcrumb Navigation">
    <a href="{{breadcrumbUrl}}" class="Breadcrumb-item{% if segmentOne %} Breadcrumb-item--first{% endif %}">{{segmentOne}}</a>
    {% if segmentTwo %} <span class="Breadcrumb-separator">/</span> <a class="Breadcrumb-item" href="">{{segmentTwo}}</a>{% endif %}
</div><!--/.Breadcrumb-->